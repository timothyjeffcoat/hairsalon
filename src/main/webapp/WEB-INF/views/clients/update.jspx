<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<div xmlns:field="urn:jsptagdir:/WEB-INF/tags/form/fields" xmlns:form="urn:jsptagdir:/WEB-INF/tags/form" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:page="urn:jsptagdir:/WEB-INF/tags/form" version="2.0">
    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
    <jsp:output omit-xml-declaration="yes"/>
    <form:update id="fu_com_yourhairsalon_booking_form_FullClientForm" modelAttribute="clients" path="/clients?update" versionField="Version">
        <input id="itemId" name="itemId" type="hidden" value="${itemId}"/>
        <field:input field="firstName" id="c_com_yourhairsalon_booking_form_FullClientForm_firstName" max="30" min="1" required="true"/>
        <field:input field="lastName" id="c_com_yourhairsalon_booking_form_FullClientForm_lastName" max="30" min="1" required="true"/>
        <field:datetime dateTimePattern="${clients_birthday_date_format}" field="birthDay" id="c_com_yourhairsalon_booking_form_FullClientForm_birthDay"/>
        <field:textarea field="address1" id="c_com_yourhairsalon_booking_form_FullClientForm_address1"/>
        <field:textarea field="address2" id="c_com_yourhairsalon_booking_form_FullClientForm_address2"/>
        <field:input field="citycode" id="c_com_yourhairsalon_booking_form_FullClientForm_citycode" max="30"/>
        <field:input field="statecode" id="c_com_yourhairsalon_booking_form_FullClientForm_statecode" max="30"/>
        <field:input field="zipcode" id="c_com_yourhairsalon_booking_form_FullClientForm_zipcode" max="5" min="5"/>
        <field:input  label="Home phone" field="home_phone" id="c_com_yourhairsalon_booking_form_FullClientForm_home_phone"/>
		<field:input  label="Work phone" field="work_phone" id="c_com_yourhairsalon_booking_form_FullClientForm_work_phone"/>
		<field:input  label="Cell phone" field="cell_phone" id="c_com_yourhairsalon_booking_form_FullClientForm_cell_phone"/>
		<field:input  label="Email" field="email" id="c_com_yourhairsalon_booking_form_FullClientForm_email"/>
    </form:update>
</div>

